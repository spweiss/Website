          if ($(this).scrollTop() < $(".page").height() * 1.70) {
             $("div").removeClass("changeColorTan")
          }
          if ($(this).scrollTop() > $(".page").height() * 2.70) {
             $("div").addClass("changeColorGrey")
          }
          if ($(this).scrollTop() < $(".page").height() * 2.70) {
             $("div").removeClass("changeColorGrey")
          }
       });
    });
    </script>
