    <script>
    $(function() {
       $(window).scroll(function () {
          if ($(this).scrollTop() > $(".page").height() * 0.70) {
             $("div").addClass("changeColorWhite")
          }
          if ($(this).scrollTop() < $(".page").height() * 0.70) {
             $("div").removeClass("changeColorWhite")
          }
          if ($(this).scrollTop() > $(".page").height() * 1.70) {
             $("div").addClass("changeColorTan")
